{
  "name": "yams_ghidra",
  "version": "0.0.1",
  "transport": "external",
  "interfaces": [
    "ghidra_analysis_v1"
  ],
  "entry": {
    "cmd": [
      "${plugin_dir}/yams_ghidra_launcher.sh"
    ],
    "env": {
      "PYTHONUNBUFFERED": "1"
    },
    "requires": [
      "python3>=3.8",
      "pyghidra>=3.0.0",
      "java>=17"
    ],
    "notes": "Set GHIDRA_INSTALL_DIR or pass ghidra_install in plugin.init config"
  }
}
