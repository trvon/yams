[settings]
os=Windows
arch=x86_64
compiler=msvc
# MSVC 194 = VS 2022 17.10+ (14.4x toolset), compatible with Boost 1.85
# Note: 193 = VS 2022 17.0-17.9 (14.3x), 194 = VS 2022 17.10+ (14.4x)
compiler.version=194
compiler.runtime=dynamic
# C++23 works fine with MSVC 194
compiler.cppstd=23
build_type=Release

[options]

[conf]
tools.cmake.cmaketoolchain:generator=Ninja

[tool_requires]
